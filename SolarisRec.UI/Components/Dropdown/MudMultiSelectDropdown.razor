@using SolarisRec.UI.UIModels

<MudSelect T="DropdownItem" Label="@Label" MultiSelection="true" Variant="Variant.Outlined" SelectedValuesChanged="SelectionChanged" @ref="mudSelect" Dense="Dense" Class="mt-0">
    @foreach (var item in DropdownItems)
    {
        <MudSelectItem T="DropdownItem" Value="@item">@item.Name</MudSelectItem>
    }
</MudSelect>
